    "@angular/language-service": "^9.1.4",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@nguniversal/builders": "^9.1.0",
    "@nrwl/cypress": "9.2.2",
    "@nrwl/jest": "9.2.2",
