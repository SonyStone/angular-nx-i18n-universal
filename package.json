    "@angular/platform-server": "^9.1.0",
    "@angular/router": "^9.1.4",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nguniversal/express-engine": "^9.1.0",
    "@nrwl/angular": "9.2.2",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
