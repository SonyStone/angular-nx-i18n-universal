    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "mock-browser": "^0.92.14",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  }
